package com.gaoxing.lesson02;

public class Person {
	String name;
	int age;
	public String getName() {
		System.out.println(name);
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		System.out.println(age);
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
}
