package 面向对象.内部类.局部内部类;


class Outter{
	static String Name = "hh";
	int age = 10;
	void show () {
		final String name = "Tom";
		class Inner{
			void test () {
				System.out.println(name);
			}
		}
		new Outter().show();
	}
}

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Outter ou = new Outter();
		ou.show();
		
	}

}
