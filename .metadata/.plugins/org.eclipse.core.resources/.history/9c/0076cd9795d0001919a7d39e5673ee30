package 面向对象.包装类;


// 定义主板
// 定义鼠标
// 定义键盘

class MainBoard {
	void Use(Mouse m) {
		m.swapData();
	}
	void Use(Key k) {
		k.swapData();
	}
}

class Mouse {
	void swapData() {
		System.out.println("鼠标工作");
	}
}

class Key{
	void swapData() {
		System.out.println("键盘工作");
	}
}

public class UseUsb {

	public static void main(String[] args) {
		MainBoard zb = new MainBoard();
		Mouse m = new Mouse();
		Key key = new Key();
		zb.Use(m);
		zb.Use(k);

	}

}
