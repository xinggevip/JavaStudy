package com.gaoxing.lesson01;

public class Lesson01 {
	String name;
	public Lesson01( String name ){
		name = name;
	};
	public void show(){
		System.out.println(name);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Lesson01 demo = new Lesson01("papy");
		demo.show();
	}

}
