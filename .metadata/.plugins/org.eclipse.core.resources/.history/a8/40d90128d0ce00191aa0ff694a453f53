package com.gaoxing.lesson06;

// 定义接口
// 实现接口
// 创建对象

interface Usb {
	void work();
}
class Mp3 implements Usb {
	String name = "mp3漫步者";
	public void work() {
		System.out.println(name + "播放了");
	}
}

class Disk implements Usb{
	String name = "硬盘东芝";
	public void work() {
		System.out.println(name + "读写正常");
	}
}

public class Testinterface {
	public static void main(String[] args) {
		Mp3 m1 = new Mp3();
		m1.work();
		Disk d1 = new Disk();
		d1.work();
		
	}
}
