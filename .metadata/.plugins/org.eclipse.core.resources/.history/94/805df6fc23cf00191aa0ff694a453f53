package 面向对象.包装类;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a = 10;
		Integer num1 = new Integer(a);
		Integer num2 = Integer.valueOf(a);
		
		
		System.out.println(num1.TYPE);// 知道是什么数据类型包装过来的
		System.out.println(num2);
		
		// 类型转换
		String str1 = "123";
		Integer num3 = new Integer(str1);
		System.out.println(num3.TYPE);
		
		

	}

}
